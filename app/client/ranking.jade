template(name="ranking")
    #ranking
        h2 Ranking
        table
            tr
                th Player
                th.score Score
            each topScores
                tr
                    td.username= username
                    td.score= pointsTotal

        table
            tr
                th Player
                th.score Longest streak
            each topStreaks
                tr
                    td.username= username
                    td.score= longestStreak
